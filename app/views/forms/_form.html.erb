<%= form_with( model: locals[:posts].current_user.posts , url: "/users/#{locals[:posts].current_user.id}/posts") do |form| %>
  <%= form.text_field :title , placeholder: 'Title' , size:"40*5" %>
  <%= form.text_area :text , size:"80*10" , placeholder: 'Type your text here!'%>
  <%= form.submit :Publish , size:"40*5"%>
<%end%>